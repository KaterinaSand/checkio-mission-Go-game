                        ['+','+','+','W','B','W','+','+','+'],
                        ['+','+','B','W','B','B','W','+','+'],
                        ['+','W','+','+','W','W','B','+','+'],
                        ['+','W','+','+','B','+','B','+','+'],
                        ['+','W','+','B','W','B','W','B','+'],
